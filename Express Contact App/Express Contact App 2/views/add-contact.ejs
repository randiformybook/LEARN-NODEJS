<div class="container mt-3">
  <div class="row">
    <div class="col-md-6">
      <h2 class="contact-list">Form Add List contact</h2>
      <form method="post" action="/contact">
        <!-- Label Nama -->
        <div class="mb-3">
          <label for="nama" class="form-label">Masukan Nama</label>
          <input
            type="text"
            class="form-control"
            id="nama"
            name="nama"
            placeholder=" Masukan Nama Contact"
            required
          />
        </div>
        <!-- Label No HP -->
        <div class="mb-3">
          <label for="nohp" class="form-label">Masukan No HP</label>
          <input
            type="text"
            class="form-control"
            id="nohp"
            name="nohp"
            placeholder=" Masukan No HP Contact"
            required
          />
        </div>
        <!-- Label Email -->
        <div class="mb-3">
          <label for="email" class="form-label">Masukan Email</label>
          <input
            type="email"
            class="form-control"
            id="email"
            name="email"
            placeholder=" Masukan Email Contact (Optional)"
          />
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    </div>
  </div>
</div>
